<body>

  <div id="wrapper">
    <div id="header" class="content-block header-wrapper">
      <div class="header-wrapper-inner">
        <section class="top clearfix" action="/login" method="POST">
          <div class="center">
            <i class="fa fa-user user-icon pull-left" href="index.ejs"></i>
            <a class="logo" href="index.ejs">Bucket</a>
            <i class="fa fa-sign-in user-icon pull-right" href="index.ejs"></i>
          </div>
        </section>
        <section class="center">
          <div class="slogan" style="margin-bottom: 100px;">
            Procrastinate Proactively
          </div>
        </section>
        <section class="bottom">
          <a id="scrollToContent" href="#">
            <img src="assets/images/arrow_down.png">
          </a>
        </section>
      </div>
    </div>

    <div class="content-block" id="portfolio">
      <div class="container">
        <div class="col-sm-4"></div>
        <div class="col-sm-4">
          <form class="form-search">
            <h2 align="center" class="form-search-heading">Enter Your Query</h2>
            <label for="inputSearch" class="sr-only">Search</label>
            <input type="text" id="inputSearch" class="form-control" placeholder="Search Movie, Song, or Restaurant">
            <label for="inputComment" class="sr-only">Comment</label>
            <input type="text" id="inputComment" class="form-control" placeholder="Add Comment">
            <button class="btn btn-lg btn-primary btn-block" type="submit">Submit</button>
          </form>
        </div>
      </div>

    <div class="container">
      <section class="block-body">
        <div class="container text-center">
          <div class="row">
            <div class="col-sm-12">
              <div class="wrapper_bu" style="position:relative;">
                <div class="image">
                  <a class="panel-heading accordion-toggle collapsed left" data-toggle="collapse"
                  data-parent="#accordion1,#accordion2,#accordion3" data-target="#collapseTwo">
                  <div id="bu1">
                    <img alt="" src="/icons/Restaraunt.png" class="img-fluid imgtransform">
                    <div class="title" style="color: #4da7ca">Restaurants</div>
                  </div>
                </a>
              </div>
              <div class="image">
                <a class="panel-heading accordion-toggle collapsed left" data-toggle="collapse"
                data-parent="#accordion1,#accordion2,#accordion3" data-target="#collapseOne">
                <div id="bu2">
                  <img alt="" src="/icons/video.png" class="img-fluid imgtransform">
                  <div class="title" style="color: #00cc7a">Films</div>
                </div>
              </a>
            </div>
            <div class="image">
              <a class="panel-heading accordion-toggle collapsed left " data-toggle="collapse" data-parent="#accordion1,#accordion2,#accordion3"
              data-target="#collapseThree">
              <div id="bu3">
                <img alt="" src="/icons/music.png" class="img-fluid imgtransform">
                <div class="title" style="color: #ff4d4d">Songs</div>
              </div>
            </a>
          </div>
        </div>
        <div class="panel-group" id="accordion1">
          <div class="panel panel-default">
            <div id="collapseOne" class="panel-collapse collapse">
              <div class="panel-body" style="color: #6a6a6a; background-color: #f1f0f0">
                <div class="center-down-arrow">
                  <p style="color: #00cc7a">Your Added Films</p>
                </div>
                <div>
                  <img src="/icons/arrow.png">
                </div>
              </div>
            </div>
          </div>
          <div class="panel panel-default">
            <div id="collapseTwo" class="panel-collapse collapse in">
              <div class="panel-body" style="color: #6a6a6a; background-color: #f1f0f0">
                <div class="center-down-arrow">
                  <p style="color: #4da7ca">Your Added Restaurants</p>
                </div>
                <div>
                  <img src="/icons/arrow.png">
                </div>
              </div>
            </div>
          </div>
          <div class="panel panel-default">
            <div id="collapseThree" class="panel-collapse collapse">
              <div class="panel-body" style="color: #6a6a6a; background-color: #f1f0f0">
                <div class="center-down-arrow">
                  <p style="color: #ff4d4d">Your Added Songs</p>
                </div>
                <div>
                  <img src="/icons/arrow.png">
                </div>
                <br>
              </br>
              <div>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <p>Hello World</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
</section>
</div>
</div>
</div>

<script src="assets/js/bootstrap.min.js"></script>
<script src="assets/js/script.js"></script>
